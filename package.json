{
	"name": "@aicacia/fund-parents",
	"version": "0.1.0",
	"description": "",
	"author": {
		"name": "Nathan Faucett",
		"email": "nathanfaucett@gmail.com"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"license": "MIT",
	"funding": {
		"type": "patreon",
		"url": "https://www.patreon.com/fund-parents"
	},
	"scripts": {
		"prisma": "prisma",
		"vite": "vite",
		"svelte-kit": "svelte-kit",
		"sync": "svelte-kit sync",
		"clean": "rm -rf node_modules/.vite .svelte-kit build",
		"start": "vite dev --host --open",
		"build": "vite build",
		"preview": "vite preview",
		"postgres": "cd postgres && docker compose up -d",
		"postgres.delete": "cd postgres && docker compose down",
		"docker.build": "docker build -t ghcr.io/fund-parents-app/fund-parents-app/fund-parents-app:latest .",
		"docker.push": "docker push ghcr.io/fund-parents-app/fund-parents-app/fund-parents-app:latest",
		"docker.run": "docker run --name fund-parents-app --rm -it -p 3000:3000 ghcr.io/fund-parents-app/fund-parents-app/fund-parents-app:latest",
		"docker.stop": "docker stop fund-parents-app",
		"helm.delete": "helm delete -n ui fund-parents-app",
		"helm.upgrade": "helm upgrade fund-parents-app helm/fund-parents-app -n ui --install --set image.hash=\"$(docker inspect --format='{{index .Id}}' ghcr.io/fund-parents-app/fund-parents-app/fund-parents-app:latest)\",env.JWT_SECRET_KEY=$JWT_SECRET_KEY,env.GOOGLE_OAUTH_CLIENT_ID=$GOOGLE_OAUTH_CLIENT_ID,env.GOOGLE_OAUTH_CLIENT_SECRET=$GOOGLE_OAUTH_CLIENT_SECRET,env.DATABASE_URL=$DATABASE_URL,env.S3_ACCESS_KEY=$S3_ACCESS_KEY,env.S3_SECRET_KEY=$S3_SECRET_KEY,env.S3_BUCKET=$S3_BUCKET,env.S3_REGION=$S3_REGION,postgresql.postgresqlUsername=\"fund-parents\",postgres.postgresqlPassword=$DATABASE_PASSWORD",
		"helm": "npm run docker.build && npm run docker.push && npm run helm.upgrade",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"secret": "node -e \"console.log(require('crypto').randomBytes(64).toString('base64'));\""
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "next",
		"@types/cookie": "^0.5.1",
		"@types/js-cookie": "^3.0.2",
		"@types/jsonwebtoken": "^8.5.8",
		"@types/prismjs": "^1.26.0",
		"@typescript-eslint/eslint-plugin": "^5.33.1",
		"@typescript-eslint/parser": "^5.33.1",
		"autoprefixer": "^10.4.8",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"postcss-import": "^14.1.0",
		"postcss-nesting": "^10.1.10",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"prisma": "^4.2.1",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	},
	"dependencies": {
		"@aicacia/debounce": "^0.1.16",
		"@aicacia/json": "^0.1.15",
		"@aicacia/rand": "^0.2.11",
		"@aicacia/range": "^0.1.3",
		"@aws-sdk/client-s3": "^3.154.0",
		"@prisma/client": "^4.2.1",
		"@stripe/stripe-js": "^1.35.0",
		"@sveltejs/kit": "next",
		"cookie": "^0.5.0",
		"eventemitter3": "^4.0.7",
		"js-cookie": "^3.0.1",
		"jsonwebtoken": "^8.5.1",
		"leaflet": "^1.8.0",
		"prismjs": "^1.28.0",
		"query-string": "^7.1.1",
		"stripe": "^10.3.0",
		"svelte": "^3.49.0",
		"svelte-icons": "^2.1.0",
		"svelte-localstorage-writable": "^0.1.0",
		"svelte-portal": "^2.2.0",
		"vest": "^4.6.4"
	}
}
